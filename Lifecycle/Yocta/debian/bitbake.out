	if [ "${IMAGE_BUILDING_DEBUGFS}" != "true" ]; then
		if [ ! -L /home/andrew/poky/build/tmp/work/qemux86_64-poky-linux/core-image-minimal/1.0-r0/rootfs/init ] && [ ! -e /home/andrew/poky/build/tmp/work/qemux86_64-poky-linux/core-image-minimal/1.0-r0/rootfs/init ]; then
