sstate_create_package() {
	# Exit early if it already exists
	if [ -e /home/andrew/poky/build/sstate-cache/ ]; then
		touch /home/andrew/poky/build/sstate-cache/ 2>/dev/null || true
		return
	fi

	mkdir --mode=0775 -p `dirname /home/andrew/poky/build/sstate-cache/`
	TFILE=`mktemp /home/andrew/poky/build/sstate-cache/.XXXXXXXX`

	OPT="-cS"
	ZSTD="zstd -8 -T4"
	# Use pzstd if available
	if [ -x "$(command -v pzstd)" ]; then
		ZSTD="pzstd -8 -p 4"
	fi

	# Need to handle empty directories
	if [ "$(ls -A)" ]; then
		set +e
		tar -I "$ZSTD" $OPT -f $TFILE *
		ret=$?
		if [ $ret -ne 0 ] && [ $ret -ne 1 ]; then
			exit 1
		fi
		set -e
	else
		tar -I "$ZSTD" $OPT --file=$TFILE --files-from=/dev/null
	fi
	chmod 0664 $TFILE
	# Skip if it was already created by some other process
	if [ -h /home/andrew/poky/build/sstate-cache/ ] && [ ! -e /home/andrew/poky/build/sstate-cache/ ]; then
		# There is a symbolic link, but it links to nothing.
		# Forcefully replace it with the new file.
		ln -f $TFILE /home/andrew/poky/build/sstate-cache/ || true
	elif [ ! -e /home/andrew/poky/build/sstate-cache/ ]; then
		# Move into place using ln to attempt an atomic op.
		# Abort if it already exists
		ln $TFILE /home/andrew/poky/build/sstate-cache/ || true
	else
		touch /home/andrew/poky/build/sstate-cache/ 2>/dev/null || true
	fi
	rm $TFILE
}

